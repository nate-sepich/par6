# Par6 Golf Environment Variables
# Copy to .env and fill in your values

# AWS Configuration
AWS_REGION=us-east-1
AWS_PROFILE=default

# Environment
ENVIRONMENT=dev

# API Configuration
API_BASE_URL=https://your-api-id.execute-api.us-west-2.amazonaws.com/dev

# Database (for future DynamoDB integration)
DYNAMODB_TABLE_NAME=par6-data-dev

# Monitoring (optional)
LOG_LEVEL=INFO